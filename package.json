{
  "name": "Krecik---gra",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "client:install": "cd client/app && npm install",
    "server:install": "cd server && npm install",
    "client:build": "cd client/app && npm run build",

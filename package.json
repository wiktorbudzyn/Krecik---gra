{
    "name": "Krecik---gra",
    "version": "1.0.0",
    "private": true,
    "scripts": {
      "start": "node server/server.js",
      "client:prebuild": "ls -la ./client/app/node_modules/.bin/ && chmod +x ./client/app/node_modules/.bin/*",
      "client:build": "npx react-scripts build --prefix client/app",
      "build": "npm run client:prebuild && npm run client:build",
      "install": "npm install --unsafe-perm"
    },
    "dependencies": {
      "axios": "^1.5.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.17.0",
      "react-scripts": "^5.0.1",
      "web-vitals": "^2.1.4",
      "express": "^4.17.1",
      "mongoose": "^5.13.13",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "express-session": "^1.17.2"
    },
    "devDependencies": {
      "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
      "cross-env": "^7.0.3"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }
  
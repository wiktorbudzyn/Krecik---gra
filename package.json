    "build": "npm run client:build"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "mongoose": "^5.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "es-abstract": "^1.23.3",
    "schema-utils": "^2.7.1",
    "semver": "^7.6.2",
    "string.prototype.matchall": "^4.0.11"
  }
}
